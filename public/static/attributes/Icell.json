{
  "classId": "idm.component.mobile.icell",
  "comName": "单元格",
  "className": "Icell",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": "",
          "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "标题文字",
            "desc": "标题文字",
            "bindKey": "titleFont",
            "default": "单元格"
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "是否居右",
            "desc": "文字是否居右",
            "bindKey": "isFontRight",
            "default": false
          },
          {
            "type": "group",
            "text": "右侧图标",
            "desc": "设置标题图标属性，颜色会覆盖主题色",
            "children": [
                {
                    "type": "iconSelect",
                    "layoutType": "inline",
                    "text": "选择图标",
                    "bindKey": "titleIcon",
                    "default": ""
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "desc": "设置图标颜色，会覆盖主题色",
                    "text": "图标颜色",
                    "bindKey": "titleIconFontColor",
                    "default": {}
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "图标大小",
                    "bindKey": "titleIconFontSize",
                    "default": 18
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "leftIconBox",
                    "default": {
                        "marginTopVal": "",
                        "marginRightVal": "",
                        "marginBottomVal": "",
                        "marginLeftVal": "0",
                        "paddingTopVal": "",
                        "paddingRightVal": "",
                        "paddingBottomVal": "",
                        "paddingLeftVal": ""
                    }
                }
            ]
          }
        ]
      },
      {
        "type": "group",
        "text": "内层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "font",
            "layoutType": "block",
            "text": "左侧字体",
            "bindKey": "leftFont"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "右侧字体",
            "bindKey": "rightFont"
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "width",
                "width": "60px",
                "default": "auto"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "height",
                "width": "60px",
                "default": "auto"
              }
            ]
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "ulbox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "",
              "paddingBottomVal": "",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
              "hex8": "#FFFFFFFF"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": "0px 0px 10px 0px #ccc"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "边框",
            "bindKey": "boxborder"
          }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "参数、消息过滤函数",
            "desc": "参数、消息过滤函数",
            "bindKey": "paramsMsgFun",
            "multiple": false
          },
          {
            "type": "input",
            "layoutType": "block",
            "text": "接收消息key",
            "desc": "接收消息key, 组件收到消息后 会传给自定义函数里，在自定义函数里可以处理参数",
            "bindKey": "messageKey"
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "点击执行函数",
            "desc": "点击执行函数",
            "bindKey": "customFunc",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "收到消息后执行函数",
            "desc": "收到消息后执行函数",
            "bindKey": "getMsgHandle",
            "multiple": false
          }
        ]
      }
  ]
}