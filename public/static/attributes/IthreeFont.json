{
  "classId": "idm.component.mobile.threeFont",
  "comName": "三行文字数据",
  "className": "IthreeFont",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "标题栏",
          "desc": "标题栏是否显示",
          "bindKey": "isTips",
          "default": true
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "标题文字",
          "desc": "标题文字",
          "bindKey": "titleDesc",
          "default": "基层党组织",
          "display": "@[isTips]"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "总个数",
          "desc": "总个数是否显示",
          "bindKey": "isNumberFlag",
          "display": "@[isTips]",
          "default": true
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "总个数",
          "desc": "总个数",
          "bindKey": "allNumberVal",
          "default": "213",
          "display": "@[isTips]"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "个数单位",
          "desc": "个数单位",
          "bindKey": "numberGe",
          "default": "个",
          "display": "@[isTips]"
        },
        {
          "type": "table",
          "text": "页签选项设置",
          "bindKey": "tableMenu",
          "tabText": "页签[index]",
          "children": [
            {
              "type": "input",
              "layoutType": "inline",
              "text": "tab标识",
              "bindKey": "key"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器宽度",
              "desc": "容器宽度，可以随意设置，记得带上单位",
              "bindKey": "liWidth",
              "default": "32%"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "容器高度",
              "desc": "容器高度，可以随意设置，记得带上单位",
              "bindKey": "liHeight",
              "default": "100px"
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "desc": "每个容器的边距",
              "bindKey": "libox",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "20px",
                "paddingRightVal": "20px",
                "paddingBottomVal": "20px",
                "paddingLeftVal": "10px"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "标题",
              "desc": "容器的标题",
              "bindKey": "title"
            },
            {
              "type": "dataSourceFiledSelect",
              "layoutType": "inline",
              "text": "标题字段",
              "desc": "标题绑定字段",
              "bindKey": "titleField"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "desc": "个数数字",
              "text": "个数",
              "bindKey": "titleSub"
            },
            {
              "type": "dataSourceFiledSelect",
              "layoutType": "inline",
              "text": "个数字段",
              "desc": "个数绑定字段",
              "bindKey": "numField"
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "border"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {
                "hex8": "#FFFFFFFF"
              }
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "文字间距",
              "desc": "文字距上距离",
              "bindKey": "titleTop",
              "default": "10px"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "标题字体",
              "bindKey": "tableTitleFont"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "个数字体",
              "bindKey": "subFont"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "样式设置",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "inlineGroup",
          "text": "标题宽高",
          "children": [
            {
              "type": "input",
              "layoutType": "block",
              "text": "标题栏宽",
              "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
              "bindKey": "titleWidth",
              "width": "60px",
              "default": "100%"
            },
            {
              "type": "input",
              "layoutType": "block",
              "text": "标题栏高",
              "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
              "bindKey": "titleHeight",
              "width": "60px",
              "default": "45px"
            }
          ]
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "标题字体",
          "bindKey": "titleFont"
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "个数字体",
          "bindKey": "numberFont"
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "单位字体",
          "bindKey": "unitFont"
        }
      ]
    },
    {
      "type": "group",
      "text": "外层样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "inlineGroup",
          "text": "宽高",
          "children": [
            {
              "type": "input",
              "layoutType": "block",
              "text": "宽",
              "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
              "bindKey": "width",
              "width": "60px",
              "default": "auto"
            },
            {
              "type": "input",
              "layoutType": "block",
              "text": "高",
              "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
              "bindKey": "height",
              "width": "60px",
              "default": "auto"
            }
          ]
        },
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "ulbox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "",
            "paddingRightVal": "",
            "paddingBottomVal": "",
            "paddingLeftVal": ""
          }
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "text": "背景色",
          "bindKey": "bgColor",
          "placeholder": "请选择颜色",
          "default": {
            "hex8": "#FFFFFFFF"
          }
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "容器阴影",
          "bindKey": "boxShadow",
          "default": "0px 0px 10px 0px #ccc"
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "boxborder"
        }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "请求参数自定义函数",
          "desc": "请求参数自定义函数",
          "bindKey": "customFunc",
          "multiple": false
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "inline",
          "text": "数据源",
          "desc": "用于选择数据源",
          "bindKey": "dataSource"
        },
        {
          "type": "group",
          "text": "字段设置",
          "desc": "设置组件中展示的对应字段",
          "children": [
            {
              "type": "dataSourceFiledSelect",
              "layoutType": "inline",
              "text": "列表字段",
              "desc": "列表字段",
              "bindKey": "listFiled",
              "default": "list"
            },
            {
              "type": "dataSourceFiledSelect",
              "layoutType": "inline",
              "text": "总个数",
              "desc": "总个数字段",
              "bindKey": "allNumberField",
              "default": "allNumber"
            }
          ]
        }
      ]
    }
  ]
}