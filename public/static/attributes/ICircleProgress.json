{
  "classId": "idm.component.mobile.iCircleProgress",
  "comName": "环形进度条",
  "className": "ICircleProgress",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "圆环直径",
          "desc": "圆环直径",
          "bindKey": "width",
          "default": "70px"
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "目标进度",
          "bindKey": "rate",
          "default": 30
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "desc": "填充颜色",
          "text": "填充颜色",
          "bindKey": "fillColor"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "顺时针",
          "desc": "是否顺时针增加",
          "bindKey": "clockwise",
          "default": true
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "进度条宽度",
          "bindKey": "proWidth",
          "default": 80
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "desc": "进度条颜色",
          "text": "进度条颜色",
          "bindKey": "proColor"
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "desc": "圆环颜色",
          "text": "圆环轨道颜色",
          "bindKey": "layerColor"
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "文字样式",
          "bindKey": "titleFont"
        }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
        {
          "type": "select",
          "layoutType": "inline",
          "text": "获取内容",
          "desc": "数据源区分：页面接口和数据源",
          "bindKey": "dataSourceType",
          "dictionary": [
            {
              "label": "数据源",
              "value": "dataSource"
            },
            {
              "label": "页面接口",
              "value": "pageInterface"
            }
          ],
          "default": "dataSource"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "结果集名",
          "bindKey": "dataName",
          "desc": "页面接口设定的结果集名称，位置为：页面设置》高级设置》页面接口",
          "default": "",
          "placeholder": "示例：resultData",
          "display": "@[dataSourceType=='pageInterface']"
        },
        {
            "type": "textarea",
            "layoutType": "inline",
            "text": "显示字段",
            "bindKey": "dataFiled",
            "desc": "根据接口返回数据格式指定结果集的字段，比如结果集名为resultData（自定义接口忽略）且它的值为{data:{filedName:[{\"text\":\"\",\"value\":\"\",\"check\":true}]}}，则这里应该填写data.filedName",
            "default": "",
            "placeholder": "示例：data.filedName 或者 [0].data[0].filedName",
            "display": "@[dataSourceType=='pageInterface']"
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "inline",
          "text": "数据源",
          "desc": "接口返回图片渲染",
          "bindKey": "dataSource",
          "display": "@[dataSourceType=='dataSource']"
        },
        {
          "type": "group",
          "text": "字段设置",
          "desc": "页面数据绑定",
          "children": [
            {
              "type": "dataSourceFiledSelect",
              "layoutType": "inline",
              "text": "进度",
              "bindKey": "progress"
            }
          ]
        }
      ]
    }
  ]
}