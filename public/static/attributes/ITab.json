{
  "classId": "idm.component.mobile.itab",
  "comName": "tab列表",
  "className": "ITab",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "加载模式",
            "bindKey": "isDrag",
            "dictionary": [
              {
                  "label": "正常加载",
                  "value": "normal"
              },
              {
                  "label": "加载容器",
                  "value": "container"
              }
            ],
            "default": "normal"
          },
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "风格样式",
            "bindKey": "type",
            "dictionary": [
                {
                    "label": "line",
                    "value": "line"
                },
                {
                    "label": "card",
                    "value": "card"
                }
            ],
            "default": "line"
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "切换动画",
            "desc":"是否开启切换标签内容时的转场动画",
            "bindKey": "animated",
            "default": true
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "粘性布局",
            "desc":"是否使用固定顶部 是否使用粘性布局",
            "bindKey": "sticky",
            "default": true
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "滑动切换",
            "desc":"是否开启手势左右滑动切换",
            "bindKey": "swipeable",
            "default": false
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "滚动导航",
            "desc":"是否开启滚动导航",
            "bindKey": "scrollspy",
            "default": false
          },
          {
            "type": "inputNumberUnit",
            "layoutType": "inline",
            "text": "粘性布局下吸顶时与顶部的距离",
            "desc":"粘性布局下吸顶时与顶部的距离",
            "bindKey": "offsetTop",
            "display": "@[sticky]",
            "dictionary": [
                {
                    "label": "px",
                    "value": "px"
                },
                {
                    "label": "vh",
                    "value": "vh"
                },
                {
                    "label": "rem",
                    "value": "rem"
                }
            ]
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {}
          },
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "tab宽",
                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                    "bindKey": "width",
                    "width": "60px",
                    "default": "auto"
                },
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "tab高",
                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                    "bindKey": "height",
                    "width": "60px",
                    "default": "44px"
                }
            ]
          }
        ]
      },
      {
        "type": "group",
        "text": "样式属性",
        "desc": "提供设置组件能达到展示效果的样式属性设置信息",
        "children": [
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "",
              "paddingBottomVal": "",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "文字",
            "bindKey": "tableStyleFont"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "选中文字",
            "bindKey": "tableChooseFont"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "容器边框",
            "bindKey": "border"
          },
          {
            "type": "group",
            "text": "选中底部线宽高",
            "children": [
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "宽",
                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                    "bindKey": "chooseWidth",
                    "width": "60px",
                    "default": "40px"
                },
                {
                  "type": "input",
                  "layoutType": "block",
                  "text": "高",
                  "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                  "bindKey": "chooseHeight",
                  "width": "60px",
                  "default": "3px"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "选中底部背景色",
                "bindKey": "chooseBgColor",
                "placeholder": "请选择颜色"
              }
            ]
          }
        ]
      },
      {
        "type": "table",
        "text": "页签选项设置",
        "bindKey": "staticTabPaneList",
        "tabText": "页签[index]",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "tab标识",
            "bindKey": "key"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "tab文字",
            "bindKey": "tab"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "绑定字段",
            "bindKey": "field"
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "禁用标签",
            "bindKey": "disable",
            "default": false
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "默认选中",
            "bindKey": "defaultActiveKey",
            "default": false
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "文字",
            "bindKey": "tableFont"
          }
        ],
        "default": [
          {
            "key": "1",
            "tab": "支部简介",
            "disable": false,
            "defaultActiveKey": false
          },
          {
            "key": "2",
            "tab": "支部党员",
            "disable": false,
            "defaultActiveKey": false
          }
        ]
      },
      {
        "type": "table",
        "text": "主题设置",
        "desc": "点击？查看主题设置用法指南",
        "bindKey": "themeList",
        "tabText": "主题[index]",
        "default":[
            {
                "key":"blue",
                "mainColor":{
                    "hex": "#0073CA",
                    "hex8": "#0073CAFF"
                }
            },
            {
                "key":"red",
                "mainColor":{
                    "hex": "#E21A1A",
                    "hex8": "#E21A1AFF"
                }
            },
            {
                "key":"green",
                "mainColor":{
                    "hex": "#0EAF64",
                    "hex8": "#0EAF64FF"
                }
            }
        ],
        "children": [
            {
                "type": "select",
                "layoutType": "inline",
                "text": "主题标识",
                "bindKey": "key",
                "multiple": false,
                "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
                "default":"",
                "placeholder":"例如：blue",
                "ctrlAttrObject": {
                    "mode": "combobox"
                },
                "dictionary": [
                    {
                        "label": "blue",
                        "value": "blue"
                    },
                    {
                        "label": "red",
                        "value": "red"
                    },
                    {
                        "label": "green",
                        "value": "green"
                    }
                ]
            },{
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "主要颜色",
                "bindKey": "mainColor",
                "desc":"主题的主要颜色",
                "placeholder": "请选择颜色",
                "display":"@[key]"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "接口请求",
            "desc":"接口是否请求",
            "bindKey": "requireFlag",
            "default": false
          },
          {
            "type": "treeSelect",
            "layoutType": "inline",
            "text": "数据源",
            "desc": "用于回填数据使用",
            "bindKey": "dataSource",
            "display": "@[requireFlag]",
            "dataSourceUrl": "/ctrl/dataSource/getDatasourceByGroup"
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "切换面板的回调自定义函数",
            "bindKey": "changeFunction",
            "desc": "切换面板的回调，接收参数为格式为{...url参数,customParam:自定义的,activeKey,allKey}",
            "multiple": true
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "切换标签前的回调函数",
            "desc": "切换标签前的回调函数，返回 false 可阻止切换，支持返回 Promise",
            "bindKey": "beforChange",
            "multiple": false
          },
          {
            "type": "pageModuleSelect",
            "layoutType": "block",
            "text": "tab菜单联动组件",
            "desc": "选择点击tab需要联动的组件",
            "bindKey": "triggerComponents",
            "multiple": true
          }
        ]
      }
  ]
}
    