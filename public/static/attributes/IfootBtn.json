                      "colors": {
                        "hex": "#EEE6E6",
                        "hex8": "#EEE6E6FF"
                      },
                      "style": "solid",
                      "width": 1,
                      "widthUnit": "px"
                    },
                    "top": {
                      "colors": {
                        "hex": "#EEE6E6",
                        "hex8": "#EEE6E6FF"
                      },
                      "style": "solid",
                      "width": 1,
                      "widthUnit": "px"
