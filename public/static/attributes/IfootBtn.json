              {
                "type": "actionSelect",
                "layoutType": "block",
                "text": "点击自定义函数",
                "bindKey": "customClickFunc",
                "desc": "点击自定义函数",
                "multiple": true
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "宽",
                "bindKey": "itemWidth",
                "default": "100px"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "高",
                "bindKey": "itemHeight",
                "default": "40px"
              },
              {
                "type": "font",
                "layoutType": "inline",
                "text": "字体样式",
                "bindKey": "itemFontStyle"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "背景色",
