{
  "classId": "idm.component.mobile.iPreviewFile",
  "comName": "预览文件",
  "className": "IpreviewFile",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
          "type": "input",
          "layoutType": "inline",
          "text": "唯一标识",
          "bindKey": "ctrlId",
          "disabled": true,
          "default": "@[packageid]",
          "desc": "",
          "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "select",
            "layoutType": "inline",
            "text": "渲染方式",
            "desc": "渲染方式：iframe加载、interfaceImg方式会调用接口拿到接口返回的图片进行预览",
            "bindKey": "renderStyle",
            "dictionary": [
              {
                "label": "iframe加载",
                "value": "iframe"
              },
              {
                "label": "接口返回图片渲染",
                "value": "interfaceImg"
              }
            ],
            "default": "interfaceImg"
          }
        ]
      },
      {
        "type": "group",
        "text": "样式设置",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "iframe宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "iframe宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "iframeWidth",
                "width": "60px",
                "default": "100%"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "iframe高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "iframeHeight",
                "width": "60px",
                "default": "100vh"
              }
            ]
          }
        ]
      },
      {
        "type": "group",
        "text": "外层样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "width",
                "width": "60px",
                "default": "auto"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "height",
                "width": "60px",
                "default": "auto"
              }
            ]
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "ulbox"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "渐变色",
            "desc": "背景渐变色",
            "bindKey": "bglinear",
            "default": ""
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
              "hex8": "#FFFFFFFF"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": ""
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "边框",
            "bindKey": "boxborder"
          }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "iframe加载Url",
            "desc": "iframe加载Url",
            "bindKey": "iframeUrl",
            "default": "",
            "display": "@[renderStyle=='iframe']"
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "渲染前校验",
            "desc": "渲染前校验是为了区分png 直接预览 不用调用接口，如果是word、fdf等等就调用接口, 只会阻止 渲染方式是：接口返回图片渲染",
            "bindKey": "isBefore",
            "default": false,
            "display": "@[renderStyle=='interfaceImg']"
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "校验函数",
            "desc": "渲染前校验，校验函数",
            "bindKey": "hanldeCheck",
            "display": "@[renderStyle=='interfaceImg']",
            "multiple": false
          },
          {
            "type": "dataSourceSelect",
            "layoutType": "inline",
            "text": "数据源",
            "desc": "接口返回图片渲染",
            "bindKey": "dataSource",
            "display": "@[renderStyle=='interfaceImg']"
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "iframe路径处理",
            "bindKey": "handleIframeUrl",
            "desc": "iframe路径处理，可以拿到路由参数加工url",
            "display": "@[renderStyle=='iframe']",
            "multiple": false
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "自定义参数",
            "bindKey": "handleResult",
            "desc": "接口返回图片渲染, 接口自定义参数",
            "multiple": false,
            "display": "@[renderStyle=='interfaceImg']"
          }
        ]
      }
  ]
}