{
  "classId": "idm.component.mobile.barChart",
  "comName": "柱状统计图",
  "className": "IBarChart",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示标题",
          "desc": "是否显示标题栏",
          "bindKey": "isShowTitleBar",
          "default": false
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "组件标题",
          "desc": "组件标题",
          "bindKey": "title",
          "default": "",
          "display": "@[isShowTitleBar]"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示图标",
          "desc": "配置是否显示标题右侧图标",
          "bindKey": "showIcon",
          "default": "true",
          "display": "@[isShowTitleBar]"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "加载描述",
          "desc": "用于指定组件加载中时的显示文字",
          "bindKey": "loadingDescription",
          "default": "加载中..."
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "为空描述",
          "desc": "用于指定组件无数据时的显示文字",
          "bindKey": "emptyDescription",
          "default": "暂无数据"
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "inline",
          "text": "数据源",
          "desc": "用于选择数据源",
          "bindKey": "chartDataSource",
          "dataSourceUrl":"/ctrl/dataSource/getDatasourceByGroup"
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "图表自定义函数",
          "bindKey": "chartDataCustomFunction",
          "desc": "用于格式化数据源返回的数据，参数为{urlObject:url参数,pageId:页面id,customParam:自定义的参数,resultData:原始数据,moduleObject:组件信息}",
          "multiple": false
        }
      ]
    },
    {
      "type": "group",
      "text": "图表属性设置",
      "desc": "点击？查看图表属性设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "dataSourceFiledSelect",
          "layoutType": "inline",
          "text": "X轴取值",
          "bindKey": "nameField",
          "default": "nameList",
          "ctrlAttrObject": {
            "dataSourceBindkey": "chartDataSource"
          }
        },
        {
          "type": "dataSourceFiledSelect",
          "layoutType": "inline",
          "text": "Y轴取值",
          "bindKey": "valueField",
          "default": "valueList",
          "ctrlAttrObject": {
            "dataSourceBindkey": "chartDataSource"
          }
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "柱状方向",
          "bindKey": "chartLayout",
          "dictionary": [
            {
              "label": "竖直",
              "value": "vertical"
            },
            {
              "label": "水平",
              "value": "horizontal"
            }
          ],
          "default": "vertical"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "顶部边距",
          "bindKey": "gridTop",
          "desc": "图像离容器上边缘的距离，请填入数字或百分比，给每个柱状上的数字以及图表标题预留空间，可适当增大此值",
          "default": "45"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "右侧边距",
          "bindKey": "gridRight",
          "desc": "图像离容器右边缘的距离，请填入数字或百分比，给每个柱状上的数字预留空间，可适当增大此值",
          "default": "0"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "图表标题",
          "desc": "会在图表上方显示",
          "bindKey": "chartTitle",
          "default": "标题"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示标签",
          "bindKey": "showLabel",
          "desc": "每个柱状外侧显示的数值",
          "default": true
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示背景",
          "bindKey": "showBackground",
          "desc": "显示每个柱状的背景，可设置颜色",
          "default": false
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示Y轴",
          "bindKey": "showYAxis",
          "default": true
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "Y轴数值",
          "bindKey": "showYAxisLabel",
          "display": "@[showYAxis]",
          "default": true
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示数轴",
          "bindKey": "showExtraXAxis",
          "desc": "会在X轴对侧展示数值",
          "default": false
        },
        {
          "type": "table",
          "text": "图表颜色",
          "desc": "这里可设置图表各项的颜色，支持渐变色设置",
          "bindKey": "colorList",
          "tabText": "颜色[index]",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "偏移量",
              "bindKey": "offset",
              "desc": "渐变色时，用来设置当前颜色的显示区域，请设置0到1的数字，其中0为柱状起始位置，1为柱状结束位置。若为单色则0到1间任意值。",
              "default": 0
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "颜色",
              "bindKey": "color",
              "placeholder": "请选择颜色"
            }
          ]
        },
        {
          "type": "group",
          "text": "柱状样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "input",
              "layoutType": "inline",
              "text": "柱状宽度",
              "desc": "可填百分比或数字，其中数字的单位为px",
              "bindKey": "barWidth",
              "default": "60%"
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "左上圆角",
              "bindKey": "barborderRadiusTopLeft",
              "default": 0
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "右上圆角",
              "bindKey": "barborderRadiusTopRight",
              "default": 0
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "左下圆角",
              "bindKey": "barborderRadiusBottomLeft",
              "default": 0
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "右下圆角",
              "bindKey": "barborderRadiusBottomRight",
              "default": 0
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景颜色",
              "bindKey": "barBgColor",
              "display": "@[showBackground]"
            }
          ]
        },
        {
          "type": "group",
          "text": "标题样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "display": "@[chartTitle]",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "chartTitleFontSize",
              "default": 16
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "文字字重",
              "bindKey": "chartTitleFontWeight",
              "default": "bolder"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "chartTitleFontColor"
            }
          ]
        },
        {
          "type": "group",
          "text": "标签样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "display": "@[showLabel]",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "chartLabelFontSize",
              "default": 12
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "chartLabelFontColor"
            }
          ]
        },
        {
          "type": "group",
          "text": "X轴样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "xAxisLabelFontSize",
              "default": 12
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "文字字重",
              "bindKey": "xAxisLabelFontWeight",
              "default": "normal"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "xAxisLabelFontColor"
            }
          ]
        },
        {
          "type": "group",
          "text": "数轴样式",
          "desc": "点击？查看样式设置用法指南",
          "display": "@[showExtraXAxis]",
          "helpUrl": "",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "extraXAxisLabelFontSize",
              "default": 12
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "extraXAxisLabelFontColor"
            }
          ]
        },
        {
          "type": "group",
          "text": "Y轴样式",
          "desc": "点击？查看样式设置用法指南",
          "display": "@[showYAxis]",
          "helpUrl": "",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "yAxisLabelFontSize",
              "default": 12,
              "display": "@[showYAxisLabel]"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "yAxisLabelFontColor",
              "display": "@[showYAxisLabel]"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "分割线颜色",
              "bindKey": "yAxisSplitLineColor"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "标题容器样式",
      "desc": "点击？查看样式设置用法指南",
      "display": "@[isShowTitleBar]",
      "helpUrl": "",
      "children": [
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "图标位置",
          "desc": "设置组件标题图标位置",
          "bindKey": "titleIconPosition",
          "display": "@[showIcon]",
          "dictionary": [
            {
              "label": "左边",
              "value": "left"
            },
            {
              "label": "右边",
              "value": "right"
            }
          ],
          "default": "left"
        },
        {
          "type": "iconSelect",
          "layoutType": "inline",
          "text": "自定义图标",
          "bindKey": "titleIcon",
          "display": "@[showIcon]"
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "text": "图标颜色",
          "bindKey": "titleIconColor",
          "display": "@[showIcon]"
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "图标大小",
          "bindKey": "titleIconSize",
          "display": "@[showIcon]",
          "default": 16
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "标题文字",
          "desc": "标题文字样式设置",
          "bindKey": "titleFont",
          "default": {
            "fontColors": {
              "hex": "#333333",
              "hex8": "#333333FF"
            },
            "fontSize": 16,
            "fontSizeUnit": "px",
            "fontWeight": "600"
          }
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "下拉文字",
          "desc": "下拉文字样式设置",
          "bindKey": "selectFont",
          "display": "@[isShowTitleBar&&columnsType!='none']"
        }
      ]
    },
    {
      "type": "group",
      "text": "外层容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "inlineGroup",
          "text": "宽高",
          "children": [
            {
              "type": "input",
              "layoutType": "block",
              "text": "宽",
              "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
              "bindKey": "width",
              "width": "60px",
              "default": "auto"
            },
            {
              "type": "input",
              "layoutType": "block",
              "text": "高",
              "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
              "bindKey": "height",
              "width": "60px",
              "default": "300px"
            }
          ]
        },
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "box",
          "default": {
            "marginTopVal": "5px",
            "marginRightVal": "5px",
            "marginBottomVal": "5px",
            "marginLeftVal": "5px",
            "paddingTopVal": "10px",
            "paddingRightVal": "10px",
            "paddingBottomVal": "10px",
            "paddingLeftVal": "10px"
          }
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "border"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "阴影样式",
          "desc": "可设置组件周围的阴影，写法请遵照标准box-shadow样式，示例：0px 2px 12px 0px rgba(0,0,0,0.07)",
          "bindKey": "boxShadow",
          "default": "0px 2px 12px 0px rgba(0,0,0,0.07)"
        },
        {
          "type": "group",
          "text": "背景设置",
          "desc": "点击？查看背景设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {}
            },
            {
              "type": "uploadImage",
              "layoutType": "inline",
              "text": "背景图片",
              "bindKey": "bgImgUrl",
              "placeholder": "可输入图片地址或直接上传"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "横向偏移",
              "bindKey": "positionX",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "纵向偏移",
              "bindKey": "positionY",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "背景大小",
              "bindKey": "bgSize",
              "dictionary": [
                {
                  "label": "裁切显示",
                  "value": "cover"
                },
                {
                  "label": "完全显示",
                  "value": "contain"
                },
                {
                  "label": "自定义",
                  "value": "custom"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "宽度",
              "bindKey": "bgSizeWidth",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgSize=='custom']"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "高度",
              "bindKey": "bgSizeHeight",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgSize=='custom']"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "平铺模式",
              "bindKey": "bgRepeat",
              "dictionary": [
                {
                  "label": "双向重复",
                  "value": "repeat"
                },
                {
                  "label": "水平重复",
                  "value": "repeat-x"
                },
                {
                  "label": "垂直重复",
                  "value": "repeat-y"
                },
                {
                  "label": "不重复",
                  "value": "no-repeat"
                },
                {
                  "label": "继承",
                  "value": "inherit"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "inline",
              "text": "背景模式",
              "bindKey": "bgAttachment",
              "dictionary": [
                {
                  "label": "固定",
                  "value": "fixed"
                },
                {
                  "label": "滚动",
                  "value": "scroll"
                },
                {
                  "label": "继承",
                  "value": "inherit"
                }
              ],
              "display": "@[bgImgUrl]"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "内层容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "innerBox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "10px",
            "paddingRightVal": "14px",
            "paddingBottomVal": "10px",
            "paddingLeftVal": "14px"
          }
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "innerBorder"
        },
        {
          "type": "group",
          "text": "背景设置",
          "desc": "点击？查看背景设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "innerBgColor",
              "placeholder": "请选择颜色",
              "default": {
                "hex8": "#FFFFFFFF"
              }
            },
            {
              "type": "uploadImage",
              "layoutType": "inline",
              "text": "背景图片",
              "bindKey": "innerBgImgUrl",
              "placeholder": "可输入图片地址或直接上传"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "横向偏移",
              "bindKey": "innerPositionX",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[innerBgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "纵向偏移",
              "bindKey": "innerPositionY",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[innerBgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "背景大小",
              "bindKey": "innerBgSize",
              "dictionary": [
                {
                  "label": "裁切显示",
                  "value": "cover"
                },
                {
                  "label": "完全显示",
                  "value": "contain"
                },
                {
                  "label": "自定义",
                  "value": "custom"
                }
              ],
              "display": "@[innerBgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "宽度",
              "bindKey": "innerBgSizeWidth",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[innerBgSize=='custom']"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "高度",
              "bindKey": "innerBgSizeHeight",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[innerBgSize=='custom']"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "平铺模式",
              "bindKey": "innerBgRepeat",
              "dictionary": [
                {
                  "label": "双向重复",
                  "value": "repeat"
                },
                {
                  "label": "水平重复",
                  "value": "repeat-x"
                },
                {
