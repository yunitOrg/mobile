{
  "classId": "idm.component.mobile.iheadlist",
  "comName": "头像列表",
  "className": "IHeadList",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "头像显示",
            "desc":"头像是否显示",
            "bindKey": "headImg",
            "default": true
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "标签显示",
            "desc":"标签是否显示",
            "bindKey": "isTag",
            "default": true
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "描述显示",
            "desc":"描述是否显示",
            "bindKey": "isDesc",
            "default": true
          },
          {
            "type": "group",
            "text": "右侧图标",
            "desc": "右侧图标",
            "helpUrl": "",
            "children": [
                {
                  "type": "switch",
                  "layoutType": "inline",
                  "text": "图标显示",
                  "desc":"图标是否显示",
                  "bindKey": "showIcon",
                  "default": true
                },
                {
                    "type": "iconSelect",
                    "layoutType": "inline",
                    "text": "图标",
                    "bindKey": "titleIconClass"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "desc": "设置图标颜色，会覆盖主题色",
                    "text": "图标颜色",
                    "bindKey": "titleIconFontColor"
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "图标大小",
                    "bindKey": "titleIconFontSize",
                    "default": 18
                }
            ]
          },
          {
            "type": "group",
            "text": "个人图标",
            "desc": "个人图标",
            "helpUrl": "",
            "children": [
                {
                  "type": "switch",
                  "layoutType": "inline",
                  "text": "图标显示",
                  "desc":"图标是否显示",
                  "bindKey": "authorshowIcon",
                  "default": true
                },
                {
                  "type": "inputNumber",
                  "layoutType": "inline",
                  "text": "图标大小",
                  "bindKey": "authorIconFontSize",
                  "default": 18
                },
                {
                    "type": "iconSelect",
                    "layoutType": "inline",
                    "text": "man图标",
                    "bindKey": "authorIconmaxClass"
                },
                {
                  "type": "colorPicker",
                  "layoutType": "inline",
                  "desc": "设置图标颜色，会覆盖主题色",
                  "text": "man图标颜色",
                  "bindKey": "authorIconmaxColor"
                },
                {
                  "type": "iconSelect",
                  "layoutType": "inline",
                  "text": "girl图标",
                  "bindKey": "authorIcongirlClass"
                },
                {
                  "type": "colorPicker",
                  "layoutType": "inline",
                  "desc": "设置图标颜色，会覆盖主题色",
                  "text": "girl图标颜色",
                  "bindKey": "authorIcongirlColor"
                }
            ]
          },
          {
            "type": "select",
            "layoutType": "inline",
            "text": "单项跳转",
            "bindKey": "itemJumpStyle",
            "desc": "设置点击单项跳转方式",
            "dictionary": [
                {
                    "label": "子页面打开",
                    "value": "_child"
                },
                {
                    "label": "自定义函数",
                    "value": "_custom_func"
                }
            ],
            "default": "_child"
          },
          {
            "type": "pageSelect",
            "layoutType": "inline",
            "text": "跳转页面",
            "display": "@[itemJumpStyle === '_child']",
            "bindKey": "itemPageList",
            "desc": "选择要打开的子页面"
          },
          {
            "type": "radio",
            "styleType": "radio",
            "layoutType": "inline",
            "text": "是否缓存",
            "display": "@[itemJumpStyle === '_child']",
            "bindKey": "isItemKeep",
            "desc": "点击跳转子页面时，是否缓存本页面",
            "default": true,
            "dictionary": [
                {
                    "label": "是",
                    "value": true
                },
                {
                    "label": "否",
                    "value": false
                }
            ]
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "自定义单项跳转函数",
            "display": "@[itemJumpStyle === '_custom_func']",
            "bindKey": "jumpItemCustomFunc",
            "multiple": false
          }
        ]
      },
      {
        "type": "group",
        "text": "外层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
              "hex": "#FFFFFFFF",
              "hex8": "FFFFFFFF"
            }
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "10px",
              "marginBottomVal": "",
              "marginLeftVal": "10px",
              "paddingTopVal": "",
              "paddingRightVal": "",
              "paddingBottomVal": "",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": "0px 0px 10px 0px #e3dede"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "容器边框",
            "bindKey": "boxBorder"
          }
        ]
      },
      {
        "type": "group",
        "text": "内层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "box",
            "layoutType": "block",
            "text": "列表边距",
            "bindKey": "liBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "20px",
              "paddingBottomVal": "",
              "paddingLeftVal": "20px"
            }
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "分割边距",
            "bindKey": "lipadding",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "20px",
              "paddingRightVal": "",
              "paddingBottomVal": "20px",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "分割线",
            "bindKey": "border",
            "default": {
              "border": {
                "bottom": {
                  "colors": {
                    "hex": "#ccc"
                  },
                  "style": "solid",
                  "visible": true,
                  "width": 1,
                  "widthUnit": "px"
                },
                "left": {
                  "colors": {}
                },
                "right": {
                  "colors": {}
                },
                "top": {
                  "colors": {}
                }
              },
              "radius": {
                "leftBottom": {},
                "leftTop": {},
                "rightBottom": {},
                "rightTop": {}
              }
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "标题字体",
            "bindKey": "titleStyle",
            "default": {
                "fontColors": {
                    "hex": "#000000"
                },
                "fontSize": 14,
                "fontSizeUnit": "px"
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "描述字体",
            "bindKey": "descStyle",
            "default": {
                "fontColors": {
                    "hex": "#000000"
                },
                "fontSize": 14,
                "fontSizeUnit": "px"
            }
          },
          {
            "type": "group",
            "text": "标签样式",
            "desc": "标签样式比如：字体、大小、颜色",
            "helpUrl": "",
            "children": [
              {
                "type": "font",
                "layoutType": "block",
                "text": "标题字体",
                "bindKey": "tagStyle"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "desc": "背景颜色",
                "text": "背景颜色",
                "bindKey": "tagBg",
                "default": {
                  "hex": "#F7B503",
                  "hex8": "#F7B503FF"
                }
              },
              {
                "type": "border",
                "layoutType": "block",
                "text": "边框",
                "bindKey": "tagBorder"
              }
            ]
          },
          {
            "type": "group",
            "text": "激活样式",
            "desc": "激活样式比如：字体、大小、颜色",
            "helpUrl": "",
            "children": [
              {
                "type": "font",
                "layoutType": "block",
                "text": "标题字体",
                "bindKey": "activeStyle"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "desc": "背景颜色",
                "text": "背景颜色",
                "bindKey": "activeBg",
                "default": {
                  "hex": "#44D7B6",
                  "hex8": "#44D7B6FF"
                }
              },
              {
                "type": "border",
                "layoutType": "block",
                "text": "边框",
                "bindKey": "activeBorder"
              }
            ]
          }
        ]
      },
      {
        "type": "table",
        "text": "主题设置",
        "desc": "点击？查看主题设置用法指南",
        "bindKey": "themeList",
        "tabText": "主题[index]",
        "default":[
            {
                "key":"blue",
                "mainColor":{
                    "hex": "#0073CA",
                    "hex8": "#0073CAFF"
                }
            },
            {
                "key":"red",
                "mainColor":{
                    "hex": "#E21A1A",
                    "hex8": "#E21A1AFF"
                }
            },
            {
                "key":"green",
                "mainColor":{
                    "hex": "#0EAF64",
                    "hex8": "#0EAF64FF"
                }
            }
        ],
        "children": [
            {
                "type": "select",
                "layoutType": "inline",
                "text": "主题标识",
                "bindKey": "key",
                "multiple": false,
                "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
                "default":"",
                "placeholder":"例如：blue",
                "ctrlAttrObject": {
                    "mode": "combobox"
                },
                "dictionary": [
                    {
                        "label": "blue",
                        "value": "blue"
                    },
                    {
                        "label": "red",
                        "value": "red"
                    },
                    {
                        "label": "green",
                        "value": "green"
                    }
                ]
            },{
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "主要颜色",
                "bindKey": "mainColor",
                "desc":"主题的主要颜色",
                "placeholder": "请选择颜色",
                "display":"@[key]"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "请求参数自定义函数",
            "desc": "请求参数自定义函数",
            "bindKey": "customFuncParameter",
            "multiple": false
          },
          {
            "type": "dataSourceSelect",
            "layoutType": "inline",
            "text": "数据源",
            "desc": "用于选择数据源",
            "bindKey": "dataSource"
          },
          {
            "type": "group",
            "text": "字段设置",
            "desc": "页面数据绑定",
            "children": [
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "头像字段",
                "bindKey": "ImgInterface",
                "default": "img"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "标题字段",
                "bindKey": "titleInterface",
                "default": "author"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "描述字段",
                "bindKey": "descInterface",
                "default": "desc"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "性别字段",
                "bindKey": "sexInterface",
                "default": "type == '1'"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "标签字段",
                "bindKey": "tagField",
                "default": "tag"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "激活字段",
                "bindKey": "activeField",
                "default": "active"
              }
            ]
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "右侧图标自定义函数",
            "bindKey": "clickFunction",
            "desc": "点击右侧图标自定义函数{...url参数,customParam:自定义的,activeKey,allKey}",
            "multiple": true
          }
        ]
      }
  ]
}
    