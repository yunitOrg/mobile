{
  "classId": "idm.component.mobile.selectPieChart",
  "comName": "饼状统计图",
  "className": "ISelectPieChart",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示标题",
          "desc": "是否显示标题栏",
          "bindKey": "isShowTitleBar",
          "default": "true"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "组件标题",
          "desc": "组件标题",
          "bindKey": "title",
          "default": "年度学时",
          "display": "@[isShowTitleBar]"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示图标",
          "desc": "配置是否显示标题右侧图标",
          "bindKey": "showIcon",
          "default": "true",
          "display": "@[isShowTitleBar]"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示表格",
          "bindKey": "showTable",
          "default": true
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "加载描述",
          "desc": "用于指定组件加载中时的显示文字",
          "bindKey": "loadingDescription",
          "default": "加载中..."
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "为空描述",
          "desc": "用于指定组件无数据时的显示文字",
          "bindKey": "emptyDescription",
          "default": "暂无数据"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "下拉描述",
          "desc": "用于指定下拉无选中时的显示文字，同时也是选择器的标题",
          "bindKey": "selectBtnPlaceholder",
          "default": "请选择",
          "display": "@[isShowTitleBar]"
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "下拉来源",
          "bindKey": "columnsType",
          "display": "@[isShowTitleBar]",
          "dictionary": [
            {
              "label": "数据源",
              "value": "dataSource"
            },
            {
              "label": "静态配置",
              "value": "static"
            },
            {
              "label": "没有下拉",
              "value": "none"
            }
          ],
          "default": "dataSource"
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "block",
          "text": "下拉数据源",
          "desc": "用于选择下拉的数据源",
          "bindKey": "columnsDataSource",
          "dataSourceUrl":"/ctrl/dataSource/getDatasourceByGroup",
          "display": "@[isShowTitleBar&&columnsType=='dataSource']"
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "下拉自定义函数",
          "bindKey": "columnsCustomFunction",
          "desc": "用于格式化数据源返回的数据，参数为{urlObject:url参数,pageId:页面id,customParam:自定义的参数,resultData:原始数据,moduleObject:组件信息}",
          "multiple": false,
          "display": "@[isShowTitleBar&&columnsType=='dataSource']"
        },
        {
          "type": "table",
          "text": "下拉静态配置",
          "bindKey": "columnsList",
          "tabText": "操作[index]",
          "display": "@[isShowTitleBar&&columnsType=='static']",
          "children": [
            {
              "type": "input",
              "layoutType": "inline",
              "text": "名称",
              "desc": "设置此项对应的文字描述",
              "bindKey": "text"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "取值",
              "desc": "设置此项对应的取值",
              "bindKey": "value"
            },
            {
              "type": "switch",
              "layoutType": "inline",
              "text": "是否默认",
              "desc": "设置此项是否为默认值",
              "bindKey": "isDefault"
            }
          ]
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "block",
          "text": "图表数据源",
          "desc": "用于选择图表的数据源",
          "bindKey": "chartDataSource",
          "dataSourceUrl":"/ctrl/dataSource/getDatasourceByGroup"
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "图表自定义函数",
          "bindKey": "chartDataCustomFunction",
          "desc": "用于格式化数据源返回的数据，参数为{urlObject:url参数,pageId:页面id,customParam:自定义的参数,resultData:原始数据,moduleObject:组件信息}",
          "multiple": false
        }
      ]
    },
    {
      "type": "group",
      "text": "图表属性设置",
      "desc": "点击？查看图表属性设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "图表高度",
          "desc": "注意当组件外层或内层容器有高度时，图表和表格的高度会按照此值的比例缩放。可使用px、%、vh等单位",
          "bindKey": "chartHeight",
          "default": "150px"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "图标标题",
          "desc": "会在图表上方显示",
          "bindKey": "chartTitle",
          "default": "标题"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示图例",
          "bindKey": "showLegend",
          "default": true
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示标签",
          "bindKey": "showLabel",
          "default": true
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "图表样式",
          "bindKey": "chartType",
          "dictionary": [
            {
              "label": "空心",
              "value": "hollow"
            },
            {
              "label": "实心",
              "value": "solid"
            }
          ],
          "default": "hollow"
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "空隙大小",
          "bindKey": "itemBorderWidth",
          "desc": "设置饼状图中各项间的间隔空隙，为0则没有间隔",
          "default": 0,
          "minLength": 0
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "顶部边距",
          "bindKey": "seriesTop",
          "desc": "图像离容器上边缘的距离，请填入数字或百分比，如果图表标题过大，则可适当增大此值",
          "default": "30",
          "display": "@[chartTitle]"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "底部边距",
          "bindKey": "seriesBottom",
          "desc": "图像离容器下边缘的距离，请填入数字或百分比，如果图例较多，则可适当增大此值",
          "default": "20",
          "display": "@[showLegend]"
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "颜色来源",
          "bindKey": "colorType",
          "dictionary": [
            {
              "label": "静态配置",
              "value": "static"
            },
            {
              "label": "取值字段",
              "value": "field"
            }
          ],
          "default": "static"
        },
        {
          "type": "table",
          "text": "图表颜色",
          "desc": "这里可设置图表各项的颜色，最终会按照数据源数组中的顺序加载对应的颜色",
          "bindKey": "chartColors",
          "tabText": "颜色[index]",
          "children": [
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "颜色",
              "bindKey": "color",
              "placeholder": "请选择颜色"
            }
          ],
          "display": "@[colorType=='static']"
        },
        {
          "type": "dataSourceFiledSelect",
          "layoutType": "inline",
          "text": "颜色字段",
          "desc": "可根据数据里的某字段控制图表颜色",
          "bindKey": "chartColorField",
          "display": "@[colorType=='field']",
          "ctrlAttrObject": {
            "dataSourceBindkey": "chartDataSource"
          }
        },
        {
          "type": "group",
          "text": "标题样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "display": "@[chartTitle]",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "chartTitleFontSize",
              "default": 16
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "文字字重",
              "bindKey": "chartTitleFontWeight",
              "default": "bolder"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "desc": "可填数字，或css样式支持的字符串",
              "bindKey": "chartTitleFontColor"
            }
          ]
        },
        {
          "type": "group",
          "text": "图例样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "display": "@[showLegend]",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "间隔大小",
              "bindKey": "legendItemGap",
              "default": 20
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "图标大小",
              "bindKey": "legendItemSize",
              "default": 14
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "legendFontSize",
              "default": 14
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "legendFontColor"
            }
          ]
        },
        {
          "type": "group",
          "text": "标签样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "display": "@[showLabel]",
          "children": [
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "文字大小",
              "bindKey": "chartLabelFontSize",
              "default": 12
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "文字颜色",
              "bindKey": "chartLabelFontColor"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "表格属性设置",
      "desc": "点击？查看标表格属性设置用法指南",
      "helpUrl": "",
      "display": "@[showTable]",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "表格高度",
          "desc": "注意当组件外层或内层容器有高度时，表格和图表的高度会按照此值的比例缩放。可使用px、%、vh等单位",
          "bindKey": "tableHeight",
          "default": "150px"
        },
        {
          "type": "table",
          "text": "字段设置",
          "bindKey": "tableFields",
          "tabText": "字段[index]",
          "children": [
            {
              "type": "dataSourceFiledSelect",
              "layoutType": "inline",
              "text": "取值字段",
              "desc": "设置此字段的取值",
              "bindKey": "name",
              "ctrlAttrObject": {
                "dataSourceBindkey": "chartDataSource"
              }
            },
            {
              "type": "select",
              "layoutType": "inline",
              "text": "对齐方式",
              "desc": "设置此扩展字段的对齐方式，不选择时默认中间居中，俩边分别居左或居右",
              "bindKey": "textAlign",
              "dictionary": [
                {
                  "label": "左",
                  "value": "left"
                },
                {
                  "label": "中",
                  "value": "center"
                },
                {
                  "label": "右",
                  "value": "right"
                }
              ]
            }
          ]
        },
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "tableBox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "",
            "paddingRightVal": "12%",
            "paddingBottomVal": "",
            "paddingLeftVal": "12%"
          }
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "图标大小",
          "bindKey": "tableIconSize",
          "default": 14
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "表格文字",
          "bindKey": "tableFont",
          "default": {
            "fontColors": {
              "hex": "#333333",
              "hex8": "#333333ff"
            },
            "fontSize": 14,
            "fontSizeUnit": "px"
          }
        }
      ]
    },
    {
      "type": "group",
      "text": "标题容器样式",
      "desc": "点击？查看样式设置用法指南",
      "display": "@[isShowTitleBar]",
      "helpUrl": "",
      "children": [
        {
          "type": "radio",
          "styleType": "radio",
          "layoutType": "inline",
          "text": "图标位置",
          "desc": "设置组件标题图标位置",
          "bindKey": "titleIconPosition",
          "display": "@[showIcon]",
          "dictionary": [
            {
              "label": "左边",
              "value": "left"
            },
            {
              "label": "右边",
              "value": "right"
            }
          ],
          "default": "left"
        },
        {
          "type": "iconSelect",
          "layoutType": "inline",
          "text": "自定义图标",
          "bindKey": "titleIcon",
          "display": "@[showIcon]"
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "text": "图标颜色",
          "bindKey": "titleIconColor",
          "display": "@[showIcon]"
        },
        {
          "type": "inputNumber",
          "layoutType": "inline",
          "text": "图标大小",
          "bindKey": "titleIconSize",
          "display": "@[showIcon]",
          "default": 16
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "标题文字",
          "desc": "标题文字样式设置",
          "bindKey": "titleFont",
          "default": {
            "fontColors": {
              "hex": "#333333",
              "hex8": "#333333FF"
            },
            "fontSize": 16,
            "fontSizeUnit": "px",
            "fontWeight": "600"
          }
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "下拉文字",
          "desc": "下拉文字样式设置",
          "bindKey": "selectFont",
          "display": "@[isShowTitleBar&&columnsType!='none']"
        }
      ]
    },
    {
      "type": "group",
      "text": "外层容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "inlineGroup",
          "text": "宽高",
          "children": [
            {
              "type": "input",
              "layoutType": "block",
              "text": "宽",
              "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
              "bindKey": "width",
              "width": "60px",
              "default": "auto"
            },
            {
              "type": "input",
              "layoutType": "block",
              "text": "高",
              "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
              "bindKey": "height",
              "width": "60px",
              "default": "auto"
            }
          ]
        },
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "box",
          "default": {
            "marginTopVal": "5px",
            "marginRightVal": "5px",
            "marginBottomVal": "5px",
            "marginLeftVal": "5px",
            "paddingTopVal": "10px",
            "paddingRightVal": "10px",
            "paddingBottomVal": "10px",
            "paddingLeftVal": "10px"
          }
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "border"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "阴影样式",
          "desc": "可设置组件周围的阴影，写法请遵照标准box-shadow样式，示例：0px 2px 12px 0px rgba(0,0,0,0.07)",
          "bindKey": "boxShadow",
          "default": "0px 2px 12px 0px rgba(0,0,0,0.07)"
        },
        {
          "type": "group",
          "text": "背景设置",
          "desc": "点击？查看背景设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "bgColor",
              "placeholder": "请选择颜色",
              "default": {}
            },
            {
              "type": "uploadImage",
              "layoutType": "inline",
              "text": "背景图片",
              "bindKey": "bgImgUrl",
              "placeholder": "可输入图片地址或直接上传"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "横向偏移",
              "bindKey": "positionX",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "纵向偏移",
              "bindKey": "positionY",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "背景大小",
              "bindKey": "bgSize",
              "dictionary": [
                {
                  "label": "裁切显示",
                  "value": "cover"
                },
                {
                  "label": "完全显示",
                  "value": "contain"
                },
                {
                  "label": "自定义",
                  "value": "custom"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "宽度",
              "bindKey": "bgSizeWidth",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgSize=='custom']"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
              "text": "高度",
              "bindKey": "bgSizeHeight",
              "dictionary": [
                {
                  "label": "px",
                  "value": "px"
                },
                {
                  "label": "%",
                  "value": "%"
                },
                {
                  "label": "em",
                  "value": "em"
                }
              ],
              "display": "@[bgSize=='custom']"
            },
            {
              "type": "radio",
              "layoutType": "block",
              "text": "平铺模式",
              "bindKey": "bgRepeat",
              "dictionary": [
                {
                  "label": "双向重复",
                  "value": "repeat"
                },
                {
                  "label": "水平重复",
                  "value": "repeat-x"
                },
                {
                  "label": "垂直重复",
                  "value": "repeat-y"
                },
                {
                  "label": "不重复",
                  "value": "no-repeat"
                },
                {
                  "label": "继承",
                  "value": "inherit"
                }
              ],
              "display": "@[bgImgUrl]"
            },
            {
              "type": "radio",
              "layoutType": "inline",
              "text": "背景模式",
              "bindKey": "bgAttachment",
              "dictionary": [
                {
                  "label": "固定",
                  "value": "fixed"
                },
                {
                  "label": "滚动",
                  "value": "scroll"
                },
                {
                  "label": "继承",
                  "value": "inherit"
                }
              ],
              "display": "@[bgImgUrl]"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "内层容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "innerBox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "10px",
            "paddingRightVal": "14px",
            "paddingBottomVal": "10px",
            "paddingLeftVal": "14px"
          }
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "innerBorder"
        },
        {
          "type": "group",
          "text": "背景设置",
          "desc": "点击？查看背景设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "背景色",
              "bindKey": "innerBgColor",
              "placeholder": "请选择颜色",
              "default": {
                "hex8": "#FFFFFFFF"
              }
            },
            {
              "type": "uploadImage",
              "layoutType": "inline",
              "text": "背景图片",
              "bindKey": "innerBgImgUrl",
              "placeholder": "可输入图片地址或直接上传"
            },
            {
              "type": "inputNumberUnit",
              "layoutType": "inline",
