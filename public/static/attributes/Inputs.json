{
  "classId": "idm.component.mobile.inputs",
  "comName": "输入框",
  "className": "Inputs",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "标题显示",
            "bindKey": "boxTitleShow",
            "default": false
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "标题icon",
            "bindKey": "boxIconShow",
            "default": false
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "底部按钮",
            "bindKey": "btnFootShow",
            "default": false
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "标题",
            "bindKey": "boxTitle",
            "default": "标题"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "标题字段",
            "bindKey": "titleFile",
            "default": "title"
          },
          {
            "type": "table",
            "text": "按钮选项设置",
            "bindKey": "btnTable",
            "tabText": "按钮[index]",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab标识",
                "bindKey": "key"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab文字",
                "bindKey": "tab"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "宽",
                "bindKey": "itemWidth",
                "default": "100px"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "高",
                "bindKey": "itemHeight",
                "default": "40px"
              },
              {
                "type": "font",
                "layoutType": "inline",
                "text": "字体样式",
                "bindKey": "itemFontStyle"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "背景色",
                "bindKey": "itemBg",
                "placeholder": "请选择颜色",
                "default": {
                  "hex": "#FFFFFF",
                  "hex8": "#FFFFFFFF"
                }
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "分割间距",
                "desc": "按钮距右侧的间距",
                "bindKey": "btnSplit"
              },
              {
                "type": "border",
                "layoutType": "block",
                "text": "容器边框",
                "bindKey": "itemBorder"
              },
              {
                "type": "actionSelect",
                "layoutType": "block",
                "text": "点击自定义函数",
                "bindKey": "customClickFunc",
                "desc": "点击自定义函数",
                "multiple": true
              }
            ]
          },
          {
            "type": "table",
            "text": "添加组件",
            "bindKey": "tableComponent",
            "tabText": "组件[index]",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab标识",
                "bindKey": "key"
              },
              {
                "type": "select",
                "layoutType": "inline",
                "text": "组件选择",
                "bindKey": "type",
                "dictionary": [
                  {
                      "label": "输入框",
                      "value": "input"
                  },
                  {
                      "label": "密码框",
                      "value": "password"
                  },
                  {
                      "label": "开关",
                      "value": "switch"
                  },
                  {
                      "label": "复选框",
                      "value": "checkbox"
                  },
                  {
                      "label": "多行文本",
                      "value": "textarea"
                  },
                  {
                    "label": "发送验证码",
                    "value": "sendPws"
                  },
                  {
                    "label": "日历",
                    "value": "calendar"
                  },
                  {
                    "label": "动作面板",
                    "value": "actionSheet"
                  }
                ]
              },
              {
                "type": "group",
                "text": "左侧文本",
                "desc": "提供设置组件能达到展示效果的基本属性设置信息",
                "children": [
                  {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "显示",
                    "bindKey": "labelShow",
                    "default": true
                  },
                  {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "一行显示",
                    "bindKey": "labelBlock",
                    "default": false
                  },
                  {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "显示对齐",
                    "bindKey": "showAlign",
                    "dictionary": [
                      {
                        "label": "左对齐",
                        "value": "left"
                      },
                      {
                        "label": "居中",
                        "value": "center"
                      },
                      {
                        "label": "右对齐",
                        "value": "right"
                      }
                    ],
                    "default": "center"
                  },
                  {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "宽",
                    "bindKey": "labelWidth",
                    "default": "100px"
                  },
                  {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "高",
                    "bindKey": "labelHeight",
                    "default": "auto"
                  },
                  {
                    "type": "font",
                    "layoutType": "block",
                    "text": "字体",
                    "bindKey": "labelFont"
                  },
                  {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "labelBox"
                  }
                ]
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "文本",
                "bindKey": "label",
                "default": "label"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "绑定字段",
                "bindKey": "field"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "placeholder",
                "bindKey": "placeholder"
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "禁用",
                "bindKey": "disabled",
                "default": false
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "是否必填",
                "bindKey": "required",
                "default": false
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "图片显示",
                "desc": "右侧是否显示图片，应用于我的信息头像显示",
                "bindKey": "isImg",
                "display": "@[type=='input']",
                "default": false
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "图片宽",
                "bindKey": "imgWidth",
                "display": "@[isImg]",
                "default": "30px"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "图片高",
                "bindKey": "imgHeight",
                "display": "@[isImg]",
                "default": "30px"
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "图片是否是圆形",
                "desc": "图片是否是圆形",
                "bindKey": "imgRadius",
                "display": "@[isImg]",
                "default": true
              },
              {
                "type": "actionSelect",
                "layoutType": "block",
                "text": "校验函数",
                "bindKey": "checkField",
                "desc": "请填写检验函数",
                "multiple": false
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "错误提示",
                "bindKey": "errorFont",
                "desc": "请填写错误提示语",
                "default": "请输入"
              },
              {
                "type": "select",
                "layoutType": "inline",
                "text": "输入框类型",
                "bindKey": "inputType",
                "display": "@[type=='input']",
                "dictionary": [
                  {
                    "label": "手机号键盘",
                    "value": "tel"
                  },
                  {
                    "label": "纯数字键盘",
                    "value": "digit"
                  },
                  {
                    "label": "带符号纯数字键盘",
                    "value": "number"
                  },
                  {
                    "label": "任何类型",
                    "value": "any"
                  }
                ],
                "default": "any"
              },
              {
                "type": "select",
                "layoutType": "inline",
                "text": "输入框对齐方式",
                "bindKey": "inputAlign",
                "dictionary": [
                  {
                    "label": "居中",
                    "value": "center"
                  },
                  {
                    "label": "右对齐",
                    "value": "right"
                  },
                  {
                    "label": "左对齐",
                    "value": "left"
                  }
                ],
                "default": "right"
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "高度自适应",
                "bindKey": "autosize",
                "default": true,
                "display": "@[type=='textarea']"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "最小高度",
                "bindKey": "rows",
                "default": 3,
                "display": "@[type=='textarea']"
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "显示字数统计",
                "bindKey": "showWordLimit",
                "default": true,
                "display": "@[type=='textarea']"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "最大字数",
                "bindKey": "maxlength",
                "default": 100,
                "display": "@[type=='textarea']"
              },
              {
                "type": "group",
                "text": "验证码样式",
                "layoutType": "block",
                "desc": "提供设置组件能达到展示效果的基本属性设置信息",
                "display": "@[type=='sendPws']",
                "children": [
                  {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "宽",
                    "bindKey": "pwdWidth",
                    "default": "35%",
                    "display": "@[type=='sendPws']"
                  },
                  {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "宽",
                    "bindKey": "pwdHeight",
                    "default": "30px",
                    "display": "@[type=='sendPws']"
                  },
                  {
                    "type": "font",
                    "layoutType": "block",
                    "text": "字体",
                    "bindKey": "pwdFont",
                    "display": "@[type=='sendPws']"
                  },
                  {
                    "type": "border",
                    "layoutType": "block",
                    "text": "分割线",
                    "bindKey": "pwdBorder",
                    "display": "@[type=='sendPws']"
                  }
                ]
              },
              {
                "type": "group",
                "text": "日历",
                "layoutType": "block",
                "desc": "提供设置组件能达到展示效果的基本属性设置信息",
                "display": "@[type=='calendar']",
                "children": [
                  {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "日期类型",
                    "bindKey": "dateType",
                    "display": "@[type=='calendar']",
                    "dictionary": [
                      {
                        "label": "单个日期",
                        "value": "single"
                      },
                      {
                        "label": "日期区间",
                        "value": "range"
                      },
                      {
                        "label": "多个日期",
                        "value": "multiple"
                      }
                    ],
                    "default": "single"
                  },
                  {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "自定义范围开始",
                    "bindKey": "dateRangeStart",
                    "display": "@[type=='calendar']",
                    "placeholder": "new Date(2022, 0, 1)"
                  },
                  {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "自定义范围结束",
                    "bindKey": "dateRangeEnd",
                    "display": "@[type=='calendar']",
                    "placeholder": "new Date(2022, 0, 1)"
                  },
                  {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "弹出位置",
                    "display": "@[type=='calendar']",
                    "bindKey": "datePosition",
                    "dictionary": [
                      {
                        "label": "上",
                        "value": "top"
                      },
                      {
                        "label": "右",
                        "value": "right"
                      },
                      {
                        "label": "下",
                        "value": "bottom"
                      },
                      {
                        "label": "左",
                        "value": "left"
                      }
                    ],
                    "default": "bottom"
                  }
                ]
              },
              {
                "type": "select",
                "layoutType": "inline",
                "text": "数据类型",
                "desc": "下拉数据源",
                "display": "@[type=='actionSheet']",
                "bindKey": "selectType",
                "dictionary": [
                  {
                    "label": "静态数据",
                    "value": "static"
                  },
                  {
                    "label": "接口获取",
                    "value": "interface"
                  }
                ],
                "default": "static"
              },
              {
                "type": "textarea",
                "layoutType": "inline",
                "text": "下拉数据",
                "bindKey": "selectOptions",
                "display": "@[type=='actionSheet' && selectType=='static']",
                "default": "[{ name: '全部', value: 0 }, { name: '新款商品', value: 1 }, { name: '活动商品', value: 2 }]"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "下拉数据name",
                "bindKey": "selectName",
                "display": "@[type=='actionSheet' && selectType=='interface']",
                "default": "name"
              },
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "下拉数据value",
                "bindKey": "selectValue",
                "display": "@[type=='actionSheet' && selectType=='interface']",
                "default": "value"
              },
              {
                "type": "dataSourceSelect",
                "layoutType": "inline",
                "text": "下拉数据源",
                "desc": "下拉框数据源",
                "display": "@[type=='actionSheet' && selectType=='interface']",
                "bindKey": "selectSource"
              }
            ],
            "default": [
              {
                "key": "1",
                "type": "input",
                "labelShow": true,
                "labelBlock": false,
                "labelWidth": "100px",
                "labelHeight": "auto",
                "label": "label",
                "field": "name",
                "disabled": false,
                "required": false,
                "errorFont": "请输入",
                "inputType": "number",
                "inputAlign": "right"
              }
            ]
          }
        ]
      },
      {
        "type": "group",
        "text": "外层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "图片宽高",
            "children": [
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "宽",
                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                    "bindKey": "boxWidth",
                    "width": "100%",
                    "default": "auto"
                },
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "高",
                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                    "bindKey": "boxHeight",
                    "width": "200px",
                    "default": "auto"
                }
            ]
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box"
          },
          {
            "type": "colorPicker",
            "layoutType": "block",
            "text": "背景颜色",
            "bindKey": "boxBg",
            "placeholder": "请选择颜色",
            "default": {
              "hex": "#FFFFFF",
              "hex8": "#FFFFFFFF"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "列表阴影",
            "bindKey": "boxShadow",
            "default": "0px  0px 20px 0px #ccc"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "分割线",
            "bindKey": "boxBorder"
          }
        ]
      },
      {
        "type": "group",
        "text": "底部按钮容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "display": "@[btnFootShow]",
        "children": [
          {
            "type": "group",
            "text": "按钮定位",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "top",
                "desc": "使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "btnTop",
                "default": "auto"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "right",
                "desc": "使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "btnRight",
                "default": "auto"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "bottom",
                "desc": "使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "btnBottom",
                "default": "0"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "left",
                "desc": "使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "btnLeft",
                "default": "auto"
              }
            ]
          },
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "容器宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "btnFootBoxWidth",
                "width": "60px",
                "default": "100%"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "容器高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "btnFootBoxHeight",
                "width": "60px",
                "default": "60px"
              }
            ]
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "btnFootBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "10px",
              "paddingBottomVal": "",
              "paddingLeftVal": "10px"
            }
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "btnFootBoxColor",
            "placeholder": "请选择颜色",
            "default": {
              "hex": "#FFFFFFFF",
              "hex8": "#FFFFFFFF"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "btnFootBoxShadown",
            "default": "0px 0px 10px 0px #e3dede"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "容器边框",
            "bindKey": "btnFootBoxBorder"
          }
        ]
      },
      {
        "type": "group",
        "text": "标题容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "display": "@[boxTitleShow]",
        "children": [
          {
            "type": "input",
            "layoutType": "inline",
            "text": "宽",
            "bindKey": "titleWidth",
            "default": "auto"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "高",
            "bindKey": "titleHeight",
            "default": "40px"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "标题字体",
            "bindKey": "titleFont"
          },
          {
            "type": "iconSelect",
            "layoutType": "inline",
            "text": "自定义图标",
            "bindKey": "boxtitleIcon"
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "图标颜色",
            "bindKey": "titleIconColor"
          },
          {
            "type": "inputNumber",
            "layoutType": "inline",
            "text": "图标大小",
            "bindKey": "titleIconSize",
            "default": 20
          }
        ]
      },
      {
        "type": "group",
        "text": "内层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "box",
            "layoutType": "block",
            "text": "组件间距",
            "bindKey": "componentBox"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "分割线",
            "bindKey": "componentBorder"
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "最后label是否显示border",
            "desc": "最后一个输入框是否显示border",
            "bindKey": "labelBorderShow",
            "default": false
          }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "底部按钮显示逻辑",
            "bindKey": "customBtnShow",
            "desc": "底部按钮显示逻辑",
            "multiple": false
          },
          {
            "type": "dataSourceSelect",
            "layoutType": "inline",
            "text": "form数据源",
            "desc": "用于form回填数据使用",
            "bindKey": "dataSource"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "分组标识",
            "desc": "传递给上下文组件",
            "bindKey": "formFiledKey",
            "default": "formGroupKey"
          },
          {
            "type": "pageModuleSelect",
            "layoutType": "block",
            "text": "tab菜单联动组件",
            "desc": "选择点击tab需要联动的组件",
            "bindKey": "triggerComponents",
            "multiple": true
          },
          {
            "type": "actionSelect",
